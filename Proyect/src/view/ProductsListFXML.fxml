<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="content" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProductsListFXMLController">
     <children>
      <TableView fx:id="productsTableView" layoutX="14.0" layoutY="14.0" prefHeight="325.0" prefWidth="450.0">
        <columns>
          <TableColumn fx:id="descriptionColumn" prefWidth="105.0" text="Description" />
          <TableColumn fx:id="priceColumn" prefWidth="127.0" text="Price" />
            <TableColumn fx:id="quantityColumn" prefWidth="127.0" text="Quantity" />
        </columns>
      </TableView>
      <Button fx:id="showButton" layoutX="501.0" layoutY="63.0" mnemonicParsing="false" onAction="#GoToProductDescription" text="Show Product" />
   </children>
</AnchorPane>
